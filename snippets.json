{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Begin a definition": {
		"prefix": "bdef",
		"body": [
			"\\begin{mydef}[$1]",
			"\t$2",
			"\\end{mydef}"
		],
		"description": "Creates Definition environment"
	},
	"Inserts a baselineskip": {
		"prefix": "baselineskip",
		"body" : [
			"\\\\\\[\\baselineskip]",
			"$1"
		],
		"description": "Inserts a baselineskip"
	},
	"Begin a proposition": {
		"prefix": "bprop",
		"body": [
			"\\begin{prop}[$1]",
			"\t$2",
			"\\end{prop}"
		],
		"description": "Creates Proposition environment"
	},
	"Begin a proof": {
		"prefix": "bproof",
		"body": [
			"\\begin{proof}",
			"\t$1",
			"\\end{proof}"
		],
		"description": "Creates proof environment"
	},
	"Begin a theorem": {
		"prefix": "bthm",
		"body": [
			"\\begin{thm}[$1]",
			"\t$2",
			"\\end{thm}"
		],
		"description": "Creates theorem environment"
	},
	"Gaussian density": {
		"prefix": "dnorm",
		"body": [
			"% Constant",
			"\\frac{1}{\\sqrt{2 \\pi $1 }}",
			"% Exponential",
			"\\exp",
			"\\left(",
			"-",
			"\\frac{",
				"\t \\left( $2 \\right)^{2}",
			  "}{",
			  "\t2 $1",
			  "}",
			"\\right)"
		],
		"description": "Gaussian density"
	},
}